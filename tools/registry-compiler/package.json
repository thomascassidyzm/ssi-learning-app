    "test": "ts-node src/index.ts ../../apml/core/ssi-variable-registry.apml --sql --ts --out ./test-output"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.0"
  }
}
