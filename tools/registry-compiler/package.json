{
  "name": "@ssi/registry-compiler",
  "version": "0.1.0",
  "description": "Compiles SSi Variable Registry APML to SQL and TypeScript",
  "main": "dist/index.js",
  "bin": {
    "registry-compile": "dist/index.js"
  },
  "scripts": {
