{
  "name": "@ssi/core",
  "version": "0.1.0",
  "description": "Framework-agnostic TypeScript core for SSi Language Learning",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./engine": {
      "types": "./dist/engine/index.d.ts",
      "import": "./dist/engine/index.mjs",
      "require": "./dist/engine/index.js"
    },
    "./learning": {
      "types": "./dist/learning/index.d.ts",
      "import": "./dist/learning/index.mjs",
      "require": "./dist/learning/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.mjs",
      "require": "./dist/config/index.js"
    },
    "./cache": {
      "types": "./dist/cache/index.d.ts",
      "import": "./dist/cache/index.mjs",
      "require": "./dist/cache/index.js"
    },
    "./persistence": {
      "types": "./dist/persistence/index.d.ts",
      "import": "./dist/persistence/index.mjs",
      "require": "./dist/persistence/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.86.0",
    "fake-indexeddb": "^6.2.5",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@supabase/supabase-js": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "@supabase/supabase-js": {
      "optional": true
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "idb": "^8.0.3"
  }
}
