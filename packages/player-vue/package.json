{
  "name": "player-vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "lint": "echo 'No linter configured yet'",
    "e2e": "playwright test",
    "e2e:chromium": "playwright test --project=chromium",
    "e2e:webkit": "playwright test --project=webkit",
    "e2e:iphone": "playwright test --project=iphone",
    "e2e:android": "playwright test --project=android",
    "e2e:ui": "playwright test --ui",
    "e2e:pwa": "pnpm build && PWA_TEST=1 playwright test",
    "e2e:prod": "PROD_URL=https://saysomethingin.app playwright test"
  },
  "dependencies": {
"@ssi/core": "workspace:*",
    "@supabase/supabase-js": "^2.86.0",
    "@vueuse/core": "^14.1.0",
    "d3": "^7.9.0",
    "three": "^0.182.0",
    "vue": "^3.5.24",
    "vue-router": "^4.3.0",
    "workbox-window": "^7.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.3.4",
    "sharp": "^0.34.5",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^1.0.0",
    "vue-tsc": "^2.0.6"
  }
}
